{
  "name": "runbox7",
  "homepage": "https://runbox.com",
  "bugs": {
    "url": "https://community.runbox.com"
  },
  "version": "0.1.1",
  "repository": "https://github.com/runbox/runbox7",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "ng": "ng",
    "appdev": "ng serve --disable-host-check --base-href=/appdev/",
    "rmm6dev": "ng serve --disable-host-check --base-href=/appdev/ rmm6",
    "start": "ng serve --proxy-config backend-proxy-remote.conf.js runbox7",
    "start-use-mockserver": "ng serve --aot --proxy-config backend-proxy-mock.conf.js --port 4201 runbox7",
    "mockserver": "tsc -p e2e/mockserver && node e2e/mockserver/out/mockserver.main.js",
    "buildrmm6": "ng build --prod --base-href=/app/ rmm6",
    "build": "node src/build/pre-build.js && ng build --prod --base-href=/app/; node src/build/post-build.js",
    "policy": "node policy-tests/run-all.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-e2e-server": "start-test mockserver 15000 start-use-mockserver",
    "run-cypress-tests": "cypress run",
    "cypress-e2e": "start-test start-e2e-server 4201 run-cypress-tests",
    "ci-tests": "node run-ci-tests.js",
    "lib": "node ngmakelib-conf.js",
    "libwatch": "node ngmakelib-conf.js --watch",
    "publishlib": "cd .ngmakelibtmp/dist && npm publish"
  },
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/pwa": "^0.901.1",
    "@angular/router": "9.0.0",
    "@angular/service-worker": "9.0.0",
    "@danielmoncada/angular-datetime-picker": "^9.1.1",
    "@sentry/browser": "^5.12.1",
    "@types/moment-timezone": "^0.5.12",
    "angular-calendar": "^0.28.2",
    "angular2-hotkeys": "2.1.5",
    "array-flat-polyfill": "^1.0.1",
    "base64-js": "^1.3.0",
    "core-js": "^2.6.2",
    "date-fns": "^1.30.1",
    "ical.js": "^1.3.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "npm": "^6.14.4",
    "rrule": "^2.6.0",
    "runbox-searchindex": "^0.1.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.2.2",
    "tinymce": "^5.2.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/language-service": "~9.0.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/jasmine": "~3.3.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~5.2.1",
    "cypress": "^4.2.0",
    "cypress-file-upload": "^4.0.3",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.2",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngmakelib": "^0.2.6",
    "protractor": "^5.4.4",
    "start-server-and-test": "^1.10.6",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.0.1",
    "tslib": "^1.10.0",
    "tslint": "~5.12.1",
    "tslint-no-focused-test": "^0.5.0",
    "typescript": "~3.7.5",
    "webdriver-manager": "12.1.7"
  }
}
